<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.movieticketbox.ui.Home.Home"
        android:label="Home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/fragment_booked_tickets"
        android:name="com.example.movieticketbox.ui.Ticket.BookedTicketsFragment"
        android:label="Ticket"
        tools:layout="@layout/fragment_booked_tickets" />
    <fragment
        android:id="@+id/navigation_movie"
        android:name="com.example.movieticketbox.ui.Movie.MovieFragment"
        android:label="fragment_ticket"
        tools:layout="@layout/fragment_movie" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.movieticketbox.ui.Profile.Profile"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>